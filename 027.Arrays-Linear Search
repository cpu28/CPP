#include <iostream>

using namespace std;

int main()
{
    int n, A[n], max=A[0], key;
    cout<<"Enter the array size:";
    cin>>n;
    cout<<"Enter "<<n<<" elements into array:";
    for (int i=0;i<n;i++)
    {
        cin>>A[i];
    }
    cout<<"Enter the key to be searched:";
    cin>>key;
    for (int i=0;i<n;i++)       
    {
        if (key==A[i])     
        {
            cout<<"Found at "<<i;
            return 0;
        }
    }
    cout<<" not found ";
    return 0;
}
