#include <iostream>

using namespace std;

int main()
{
    int x,y,GCD=1;
    cout<<"Enter any two numbers:";
    cin>>x>>y;
    for (int i=1;(i<=x && y);i++) //same as i<=x and i<=y
    {
        if (x%i==0 && y%i==0)
        {
            GCD=i;
        }
    }
    cout<<"GCD of "<<x<<" and "<<y<<" is "<<GCD;

    return 0;
}
