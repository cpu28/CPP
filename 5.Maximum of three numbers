#include <iostream>

using namespace std;

int main()
{
    int x,y,z;
    cout<<"Enter 3 Numbers";
    cin>>x>>y>>z;
    if (x>y && x>z)
        cout<<"Max number is "<<x;
    else
        if(y>z)
            cout<<"Max number is "<<y;
        else
            cout<<"Max number is "<<z;
    return 0;
}
